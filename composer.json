{
  "name": "harmony7/laravel-grip",
  "description": "GRIP for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuyuki Ohmuro",
      "email": "kats@fanout.io"
    }
  ],
  "require": {
    "fanout/grip": "v1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Fanout\\LaravelGrip\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fanout\\LaravelGrip\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fanout\\LaravelGrip\\LaravelGripServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5",
    "ramsey/uuid": "^4.2"
  }
}
